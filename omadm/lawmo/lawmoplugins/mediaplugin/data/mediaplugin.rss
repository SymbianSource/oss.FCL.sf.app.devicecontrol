	// UID for the DLL
	dll_uid = 0x2002EA28;
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			// UID of interface that is implemented
			interface_uid = 0x2002EA1F;
			implementations = 
				{
				// Info for LAWMO Contacts Plugin
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x2002EA29;
					version_no = 1;
					display_name = "Media";
					default_data = "text/wml";
					opaque_data = "Media";
					}
				};
			}
		};
	}

